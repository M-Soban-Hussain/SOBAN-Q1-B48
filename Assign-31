let users = ["admin","soban", "ashiq", "sami", "ali"]
users.length=0;
if(users.length==0){
    console.log("WE need to Find more Users.");
}
else{
for(let n=0; n<=4; n++){
    if(users[n]=="admin"){
        console.log("Hello admin, would you like to see a status report?");
    }
    else{
        console.log(`Hello ${users[n]}, thank you for logging in again.`)
}}}
