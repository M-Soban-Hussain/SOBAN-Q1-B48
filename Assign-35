let dangerous_animals = ["crocodiles", "snakes", "dinosaurs"];
for(let a=0; a<dangerous_animals.length; a++){
    console.log(`\b ${dangerous_animals[a]}`)
}
console.log("\n");
for(let b=0; b<dangerous_animals.length; b++){
    console.log(`${dangerous_animals[b]} are carnivores.`);
}
console.log("\n");
console.log("These all animals belong from the family of reptiles.");
